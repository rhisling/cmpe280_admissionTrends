<!--Side menu and right menu -->
<%- include('includes/sidemenu.ejs') %>

<!-- main content -->
<section class="content home">
    <div class="container-fluid">
        <div class="block-header">
            <h2>What's your admission chance?</h2>
            <small class="text-muted">Welcome to AdmissionTrends</small>
        </div>

    <header>

        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="header">
                        <h2>SAT and GPA Stats<small>Please provide information for the following fields:</small> </h2>
                        <ul class="header-dropdown">
                            <li class="dropdown"> <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="zmdi zmdi-more-vert"></i></a>
                                <ul class="dropdown-menu pull-right">
                                    <li><a href="javascript:void(0);" class=" waves-effect waves-block">Action</a></li>
                                    <li><a href="javascript:void(0);" class=" waves-effect waves-block">Another action</a></li>
                                    <li><a href="javascript:void(0);" class=" waves-effect waves-block">Something else here</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="body">

                            <div class="row clearfix">
                                <div class="col-lg-6 col-md-12">
                                    <div class="form-group">
                                        <div class="form-line">
                                            <label class="form-control" for="univ-name">SAT Score:</label>
                                            <input class="form-control col-md-12" type="number" id="SAT" name="INSTNM" value="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-12">
                                    <div class="form-group">
                                        <div class="form-line">
                                            <label class="form-control" for="GPA">GPA:</label>
                                            <input class="form-control col-md-12" type="number" id="GPA" name="ADM_RATE" step=0.0001
                                                   value="">
                                        </div>
                                    </div>

                                </div>
                            </div>




                            <div class="col-sm-12">
                                <button id="btnGpaSat" class="btn btn-raised g-bg-blush2">Submit</button>
                            </div>
                        <div class="row clearfix" style="display:none;" id="graph">
                            <div class="row">
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <div class="card">
                                    <div class="header" style="margin-top:0px; height:50px">
                                        <h2 style="text-align:center;text-transform: none ;background:linear-gradient(45deg, #dd5e89, #f7bb97); height: 45px;padding: 8px;">GPA VS SAT Scores</h2>
                                    </div>
                                    <div class="body">
                                        <canvas id="line_chart1" height="150"></canvas>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="container">
                            <div>
                            <table class="table table-striped" id="show">
                            <thead>
                            <tr style="background:linear-gradient(45deg, #dd5e89, #f7bb97);">
                            <th style="text-align: center;">Recommended Universities&nbsp&nbsp;</th>

                            </tr>
                            </thead>
                            <tbody id="univ-tbody">



                            </tbody>
                            </table>
                            </div>
                            </div>

                            </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    </div>
</section>



<!-- main content -->

<div class="color-bg"></div>



