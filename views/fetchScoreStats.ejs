<%- include('includes/header.ejs') %>
<!--Side menu and right menu -->
<%- include('includes/sidemenu.ejs') %>

<!-- main content -->
<section class="content home">
    <div class="container-fluid">
        <div class="block-header">
            <h2>Dashboard</h2>
            <small class="text-muted">Welcome to AdmissionTrends</small>
        </div>
        <div class="row">
            <div class="col-md-2 p-4" style="text-align:right">Select University:</div>
            <select class="form-control col-md-2 m-3" id="univOption" style="border: 1px solid black" required>
                <option selected value="University of California-Berkeley">UC Berkeley</option>
                <option value="University of California-Irvine">UC Irvine</option>
                <option value="University of California-Los Angeles">UCLA</option>
                <option value="University of California-Davis">UC Davis</option>
                <option value="University of California-Santa Barbara">UC Santa Barbara</option>
                <option value="University of California-Riverside">UC Riverside</option>
                <option value="University of California-San Diego">UC San Diego</option>
                <option value="University of California-Merced">UC Merced</option>
            </select>
        </div>
    </div>
    <header>

        <div class="row clearfix">

            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="card">
                    <div class="header">
                        <h2 style="text-align:center;text-transform: none">Student SAT score Midpoint</h2>
                    </div>
                    <div class="body">
                        <canvas id="line_chart1" height="150"></canvas>
                    </div>
                </div>
            </div>



        </div>
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="header">
                        <h2>SAT and GPA Stats<small>Please provide information for the following fields:</small> </h2>
                        <ul class="header-dropdown">
                            <li class="dropdown"> <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="zmdi zmdi-more-vert"></i></a>
                                <ul class="dropdown-menu pull-right">
                                    <li><a href="javascript:void(0);" class=" waves-effect waves-block">Action</a></li>
                                    <li><a href="javascript:void(0);" class=" waves-effect waves-block">Another action</a></li>
                                    <li><a href="javascript:void(0);" class=" waves-effect waves-block">Something else here</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="body">
                        <form action="/getUserScoreStats" method="GET">
                            <div class="row clearfix">
                                <div class="col-lg-6 col-md-12">
                                    <div class="form-group">
                                        <div class="form-line">
                                            <label class="form-control" for="univ-name">SAT Score:</label>
                                            <input class="form-control col-md-12" type="text" id="univ-name" name="INSTNM" value="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-12">
                                    <div class="form-group">
                                        <div class="form-line">
                                            <label class="form-control for="GPA">GPA:</label>
                                            <input class="form-control col-md-12" type="number" id="GPA" name="ADM_RATE" step=0.0001
                                                   value="">
                                        </div>
                                    </div>

                                </div>
                            </div>




                            <div class="col-sm-12">
                                <button type="submit" class="btn btn-raised g-bg-blush2">Submit</button>
                                <!--                                 <button type="submit" class="btn btn-raised btn-default ">Cancel</button>
                                 -->                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>


</header>
</section>



<!-- main content -->

<div class="color-bg"></div>



<!-- Jquery Core Js -->
<script src="assets/bundles/libscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js -->
<script src="assets/bundles/vendorscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js -->
<script src="assets/bundles/morphingsearchscripts.bundle.js"></script> <!-- Main top morphing search -->

<script src="assets/plugins/jquery-sparkline/jquery.sparkline.min.js"></script> <!-- Sparkline Plugin Js -->
<script src="assets/plugins/chartjs/Chart.bundle.min.js"></script> <!-- Chart Plugins Js -->

<script src="assets/bundles/mainscripts.bundle.js"></script><!-- Custom Js -->
<script src="assets/js/pages/charts/sparkline.min.js"></script>
<script src="gpa-graphs.js"></script>



</body>
