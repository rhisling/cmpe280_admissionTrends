<!-- main content -->
<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <h2>Delete Entry</h2>
            <small class="text-muted">Welcome to University Trends, Administrator!</small>
        </div>
        <div class="row clearfix">
			<div class="col-lg-12 col-md-12 col-sm-12">
				<div class="card">
					<div class="header">
						<h2>Delete a university entry<small>Please provide information for the following fields:</small> </h2>
						<ul class="header-dropdown">
							<li class="dropdown"> <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="zmdi zmdi-more-vert"></i></a>
								<ul class="dropdown-menu pull-right">
									<li><a href="javascript:void(0);" class=" waves-effect waves-block">Action</a></li>
									<li><a href="javascript:void(0);" class=" waves-effect waves-block">Another action</a></li>
									<li><a href="javascript:void(0);" class=" waves-effect waves-block">Something else here</a></li>
								</ul>
							</li>
						</ul>
					</div>
					<div class="body">
                    <div class="alert alert-success col-md-6" role="alert" id="success-alert-add">
                        <strong>Success!</strong> Deleted University.
                    </div>
                    <div class="alert alert-warning" role="alert" id="failure-alert-add">
                        <strong>Failure!</strong> Could not delete.
                    </div>
                        <div class="row clearfix">
                            <div class="col-lg-6 col-md-12">
                                <div class="form-group">
                                    <div class="form-line">
                    <label class="form-control" for="univ-name">University Name:</label>
                    <input class="form-control col-md-12" type="text" id="univ-name" name="INSTNM" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-12">
                                <div class="form-group">
                                    <div class="form-line">
                    <label class="form-control" for="year">Year:</label>
                    <input class="form-control col-md-12" type="number" id="year" name="YEAR" value="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row clearfix">
                            <div class="col-lg-4 col-md-6 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                    <label class="form-control for="admission-rate">Admission Rate:</label>
                    <input class="form-control col-md-12" type="number" id="admission-rate" name="ADM_RATE" step=0.0001
                        value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                    <label class="form-control" for="average-sat">Average SAT:</label>
                    <input class="form-control col-md-12" class="form-control col-md-2" type="number" id="average-sat"
                        name="SAT_AVG" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                    <label class="form-control" for="in-state-tuition">In-State Tution:</label>
                    <input class="form-control col-md-12" type="number" id="in-state-tuition" name="TUITIONFEE_IN" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12">
                                <button type="submit" id="del-submit" class="btn btn-raised g-bg-blush2">Submit</button>
<!--                                 <button type="submit" class="btn btn-raised btn-default ">Cancel</button>
 -->                            </div>
                            
                        </div>
                    </div>
				</div>
			</div>
		</div>    
    </div>
</section>

<div class="color-bg"></div>
